don't call\n    // `navigator.splashscreen.hide()`)\n    setTimeout(function () {\n      handle.release();\n    }, 6000);\n  }\n});\n\n////////////////////////////////////////////////////////////////////////////\n\n}).call(this);\n\n\n/* Exports */\nPackage._define(\"launch-screen\", {\n  LaunchScreen: LaunchScreen\n});\n\n})();\n","servePath":"/packages/launch-screen.js"}]