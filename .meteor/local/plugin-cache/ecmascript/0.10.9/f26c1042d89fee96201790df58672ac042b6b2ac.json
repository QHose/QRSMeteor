 children can legally be appended to it.\n    const container = destination.cloneNode(false); // Parse the HTML into the container, allowing for multiple children.\n\n    container.innerHTML = nodeOrHtml; // Transplant the children to the destination.\n\n    while (container.firstChild) {\n      destination.appendChild(container.firstChild);\n    }\n  } else if (Array.isArray(nodeOrHtml)) {\n    nodeOrHtml.forEach(elem => appendContent(destination, elem));\n  } else {\n    destination.appendChild(nodeOrHtml);\n  }\n}","map":{"version":3,"sources":["packages/server-render/client-sink.js"],"names":["module","export","ClientSink","doc","document","head","getElementsByTagName","body","isoError","method","console","error","appendToHead","nodeOrHtml","appendContent","appendToBody","appendToElementById","id","getElementById","renderIntoElementById","element","lastChild","removeChild","redirect","location","window","setStatusCode","setHeader","getHeaders","getCookies","destination","container","cloneNode","innerHTML","firstChild","appendChild","Array","isArray","forEach","elem"],"mappings":"AAAAA,OAAOC,MAAP,CAAc;AAACC,cAAW,MAAIA;AAAhB,CAAd;AAAA,MAAMC,MAAMC,QAAZ;AACA,MAAMC,OAAOF,IAAIG,oBAAJ,CAAyB,MAAzB,EAAiC,CAAjC,CAAb;AACA,MAAMC,OAAOJ,IAAII,IAAjB;;AAEA,MAAMC,WAAYC,MAAD,IAAY;AAC3BC,UAAQC,KAAR,CAAe,QAAOF,MAAO;4CAA7B;AAED,CAHD;;AAIO,MAAMP,UAAN,CAAiB;AACtBU,eAAaC,UAAb,EAAyB;AACvBC,kBAAcT,IAAd,EAAoBQ,UAApB;AACD;;AAEDE,eAAaF,UAAb,EAAyB;AACvBC,kBAAcP,IAAd,EAAoBM,UAApB;AACD;;AAEDG,sBAAoBC,EAApB,EAAwBJ,UAAxB,EAAoC;AAClCC,kBAAcX,IAAIe,cAAJ,CAAmBD,EAAnB,CAAd,EAAsCJ,UAAtC;AACD;;AAEDM,wBAAsBF,EAAtB,EAA0BJ,UAA1B,EAAsC;AACpC,UAAMO,UAAUjB,IAAIe,cAAJ,CAAmBD,EAAnB,CAAhB;;AACA,WAAOG,QAAQC,SAAf,EAA0B;AACxBD,cAAQE,WAAR,CAAoBF,QAAQC,SAA5B;AACD;;AACDP,kBAAcM,OAAd,EAAuBP,UAAvB;AACD;;AAEDU,WAASC,QAAT,EAAmB;AACjB;AACAC,WAAOD,QAAP,GAAkBA,QAAlB;AACD,GAxBqB,CA0BtB;;;AACAE,kBAAgB;AACdlB,aAAS,eAAT;AACD;;AAEDmB,cAAY;AACVnB,aAAS,WAAT;AACD;;AAEDoB,eAAa;AACXpB,aAAS,YAAT;AACD;;AAEDqB,eAAa;AACXrB,aAAS,YAAT;AAC