{
  "name": "qrsmeteor",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.22.15",
    "axios": "^1.2.3",
    "bluebird": "^3.5.1",
    "core-js": "^2.6.12",
    "enigma.js": "^2.12.0",
    "faker": "^4.1.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^4.0.3",
    "highlight.js": "^9.18.5",
    "ip": "^1.1.8",
    "jquery": "2.2.4",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "marked": "^4.3.0",
    "meteor-node-stubs": "^1.0.0",
    "micromodal": "^0.4.10",
    "moment": "^2.29.4",
    "request": "^2.88.2",
    "reveal.js": "^3.6.0",
    "sanitize-filename": "^1.6.1",
    "showdown": "^1.9.1",
    "simpl-schema": "^0.2.3",
    "ssl-root-cas": "^1.3.1",
    "url": "^0.11.2"
  },
  "scripts": {
    "pretest": "npm run lint --silent",
    "test": "meteor test --once --driver-package dispatch:mocha-phantomjs",
    "test-watch": "meteor test --driver-package practicalmeteor:mocha",
    "test-app-watch": "meteor test --full-app --driver-package practicalmeteor:mocha",
    "lint": "eslint .",
    "chimp-watch": "chimp --ddp=http://localhost:3000 --watch --mocha --path=tests"
  },
  "devDependencies": {},
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "plugins": [
      "meteor"
    ],
    "extends": [
      "airbnb/base",
      "plugin:meteor/recommended"
    ],
    "rules": {
      "meteor/eventmap-params": [
        2,
        {
          "eventParamName": "event",
          "templateInstanceParamName": "instance"
        }
      ],
      "meteor/template-names": [
        0
      ]
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browsers": [
          "last 2 versions"
        ]
      }
    }
  }
}
